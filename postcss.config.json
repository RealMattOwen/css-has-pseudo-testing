{
  "plugins": [
    "postcss-flexbugs-fixes",
    [
      "postcss-preset-env",
      {
        "autoprefixer": {
          "flexbox": "no-2009"
        },
        "stage": 3,
        "features": {
          "custom-properties": false,
          "has-pseudo-class": false
        }
      }
    ],
    "@csstools/css-has-pseudo-experimental"
  ]
}